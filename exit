[32m+[m[32mimport requests[m
[32m+[m[32mimport lxml[m
[32m+[m[32mfrom bs4 import BeautifulSoup[m
[32m+[m
[32m+[m[32mdef get_link_data(url):[m
[32m+[m[32m    headers = {[m
[32m+[m[32m    "User-Agent":"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/123.0.0.0 Safari/537.36",[m
[32m+[m[32m    "Acceot-Language":"en",[m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    response = requests.get(url,headers = headers)[m
[32m+[m
[32m+[m[32m    soup = BeautifulSoup(response.text,'lxml')[m
[32m+[m
[32m+[m[32m    name = soup.select_one(selector="#productTitle").getText()[m
[32m+[m[32m    name = name.strip()[m
[32m+[m
[32m+[m[32m    price = soup.select_one(selector=".a-price-whole").getText()[m
[32m+[m[32m    price = float(price.replace(',',''))[m
[32m+[m
[32m+[m[32m    return name,price[m
