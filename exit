[32m+[m[32mfrom django.db import models[m
[32m+[m[32mfrom .utils import get_link_data[m
[32m+[m[32m# Create your models here.[m
[32m+[m
[32m+[m[32mclass Link(models.Model):[m
[32m+[m[32m    name = models.CharField(max_length=220, blank=True)[m
[32m+[m[32m    url = models.URLField()[m
[32m+[m[32m    current_price = models.FloatField(blank=True)[m
[32m+[m[32m    old_price = models.FloatField(default=0)[m
[32m+[m[32m    price_difference = models.FloatField(default=0)[m
[32m+[m[32m    updated = models.DateTimeField(auto_now=True)[m
[32m+[m[32m    created = models.DateTimeField(auto_now_add=True)[m
[32m+[m
[32m+[m[32m    def __str__(self):[m
[32m+[m[32m        return str(self.name)[m
[32m+[m[41m    [m
[32m+[m[32m    class Meta:[m
[32m+[m[32m        ordering = ('price_difference','-created') #reverse order[m
[32m+[m[41m    [m
[32m+[m[32m    def save(self,*args,**kwargs):[m
[32m+[m[32m        name, price = get_link_data(self.url)[m
[32m+[m[32m        old_price = self.current_price[m
[32m+[m[32m        if self.current_price:[m
[32m+[m[32m            if price != old_price:[m
[32m+[m[32m                diff = price - old_price[m
[32m+[m[32m                self.price_difference = round(diff, 2)[m
[32m+[m[32m                self.old_price = old_price[m
[32m+[m[32m        else:[m
[32m+[m[32m            self.old_price = 0[m
[32m+[m[32m            self.price_difference = 0[m
[32m+[m
[32m+[m[32m        self.name = name[m
[32m+[m[32m        self.current_price = price[m[41m        [m
[32m+[m[32m        super().save(*args,**kwargs)[m
\ No newline at end of file[m
