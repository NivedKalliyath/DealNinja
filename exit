[1mdiff --git a/links/views.py b/links/views.py[m
[1mnew file mode 100644[m
[1mindex 0000000..0e7401b[m
[1m--- /dev/null[m
[1m+++ b/links/views.py[m
[36m@@ -0,0 +1,50 @@[m
[32m+[m[32mfrom django.shortcuts import render[m
[32m+[m[32mfrom .models import Link[m
[32m+[m[32mfrom .forms import AddLinkForm[m
[32m+[m[32m# Create your views here.[m
[32m+[m
[32m+[m[32m'''[m
[32m+[m[32mFor our home view we need to pass to the template:[m
[32m+[m[32m- qs[m
[32m+[m[32m- number of items[m
[32m+[m[32m- number of items discounted[m
[32m+[m[32m- form[m
[32m+[m[32m- error (if exists)[m
[32m+[m[32m'''[m
[32m+[m
[32m+[m[32mdef home_view(request):[m
[32m+[m[32m    no_discounted = 0[m
[32m+[m[32m    error = None[m
[32m+[m
[32m+[m[32m    form = AddLinkForm(request.POST or None)[m
[32m+[m
[32m+[m[32m    if request.method == 'POST':[m
[32m+[m[32m        try:[m
[32m+[m[32m            if form.is_valid():[m
[32m+[m[32m                form.save()[m
[32m+[m[32m        except AttributeError:[m
[32m+[m[32m            error = "Oops... Couldn't get the name/price"[m
[32m+[m[32m        except:[m
[32m+[m[32m            error = "Oops... Something went wrong"[m
[32m+[m
[32m+[m[32m    form = AddLinkForm()[m
[32m+[m
[32m+[m[32m    qs = Link.objects.all()[m
[32m+[m[32m    items_no = qs.count()[m
[32m+[m
[32m+[m[32m    if items_no > 0:[m
[32m+[m[32m        discount_list = [][m
[32m+[m[32m        for item in qs:[m
[32m+[m[32m            if item.old_price > item.current_price:[m
[32m+[m[32m                discount_list.append(item)[m
[32m+[m[32m            no_discounted = len(discount_list)[m
[32m+[m[41m    [m
[32m+[m[32m    context = {[m
[32m+[m[32m        'qs': qs,[m
[32m+[m[32m        'items_no': items_no,[m
[32m+[m[32m        'no_discounted': no_discounted,[m
[32m+[m[32m        'form': form,[m
[32m+[m[32m        'error': error,[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return render(request, 'links/main.html',context)[m
